      <element name="updateLineItem">
        <annotation>
          <documentation>
            Updates the specified {@link LineItem}.

            @param lineItem the line item to update
            @return the updated line item
          </documentation>
        </annotation>
        <complexType>
          <sequence>
            <element maxOccurs="1" minOccurs="0" name="lineItem" type="tns:LineItem"/>
          </sequence>
