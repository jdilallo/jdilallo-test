            </annotation>
          </element>
          <element maxOccurs="unbounded" minOccurs="0"
            name="deviceManufacturers" type="tns:Technology">
            <annotation>
              <documentation>
                Device manufacturers that are being targeted or excluded by the
                {@link LineItem}.
              </documentation>
            </annotation>
