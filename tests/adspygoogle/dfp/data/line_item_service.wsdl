            <annotation>
              <documentation>
                Device capabilities that are being targeted by the {@link LineItem}.
              </documentation>
            </annotation>
          </element>
          <element maxOccurs="unbounded" minOccurs="0"
            name="excludedDeviceCapabilities" type="tns:Technology">
