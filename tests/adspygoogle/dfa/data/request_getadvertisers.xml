<?xml version="1.0" encoding="UTF-8"?>
<SOAP-ENV:Envelope SOAP-ENV:encodingStyle="http://schemas.xmlsoap.org/soap/encoding/"
                   xmlns:SOAP-ENC="http://schemas.xmlsoap.org/soap/encoding/"
                   xmlns:SOAP-ENV="http://schemas.xmlsoap.org/soap/envelope/"
                   xmlns:xsd3="http://www.w3.org/2001/XMLSchema"
                   xmlns:xsi3="http://www.w3.org/2001/XMLSchema-instance">
 <SOAP-ENV:Header>
  <Security xmlns="http://docs.oasis-open.org/wss/2004/01/oasis-200401-wss-wssecurity-secext-1.0.xsd">
   <UsernameToken>
    <Username xsi3:type="xsd3:string">xxxxxx</Username>
    <Password xsi3:type="xsd3:string">xxxxxx</Password>
   </UsernameToken>
  </Security>
 </SOAP-ENV:Header>
 <SOAP-ENV:Body>
  <getAdvertisers>
   <advertiserSearchCriteria xsi3:type="AdvertiserSearchCriteria">
    <searchString/>
    <pageSize>10</pageSize>
   </advertiserSearchCriteria>
  </getAdvertisers>
 </SOAP-ENV:Body>
</SOAP-ENV:Envelope>
